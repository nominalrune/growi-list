{
  "name": "growi-list-view",
  "displayName": "growi list view",
  "description": "[unofficial] growi vscode extention",
  "version": "0.1.0",
  "publisher": "KeiYokoyama",
  "icon": "resources/icon.png",
  "repository": {
    "url": "https://github.com/nominalrune/growi-list-view"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "growi-list-view.save-token",
        "title": "Input token",
        "icon": {
          "light": "resources/key.svg",
          "dark": "resources/key.svg"
        },
        "category": "GrowiList"
      },
      {
        "command": "growi-list-view.save-url",
        "title": "Input url",
        "icon": {
          "light": "resources/key.svg",
          "dark": "resources/key.svg"
        },
        "category": "GrowiList"
      },
      {
        "command": "growi-list-view.delete-token",
        "title": "Delete token",
        "icon": "resources/key.svg",
        "category": "GrowiList"
      },
      {
        "command": "growi-list-view.delete-url",
        "title": "Delete url",
        "icon": "resources/key.svg",
        "category": "GrowiList"
      },
      {
        "command": "growi-list-view.show-page-list",
        "title": "List pages",
        "icon": "resources/sync.svg",
        "category": "GrowiList"
      },
      {
        "command": "growi-list-view.show-content",
        "title": "Show page content",
        "icon": "resources/sync.svg"
      },
      {
        "command": "growi-list-view.node.insert",
        "title": "Insert Node",
        "icon": "resources/plus.svg"
      },
      {
        "command": "growi-list-view.node.indent",
        "title": "Indent Node"
      },
      {
        "command": "growi-list-view.node.outdent",
        "title": "Outdent Node"
      },
      {
        "command": "growi-list-view.node.edit",
        "title": "Edit Node",
        "icon": "resources/edit.svg"
      },
      {
        "command": "growi-list-view.node.delete",
        "title": "Delete Node",
        "icon": "resources/x.svg"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "growi-list-view.save-token"
        },
        {
          "command": "growi-list-view.delete-token"
        },
        {
          "command": "growi-list-view.save-url"
        },
        {
          "command": "growi-list-view.delete-url"
        },
        {
          "command": "growi-list-view.show-page-list"
        },
        {
          "command": "growi-list-view.show-content"
        }
      ],
      "view/title": [
        {
          "command": "growi-list-view.show-page-list",
          "when": "view == GrowiList-page-list",
          "group": "navigation"
        },
        {
          "command": "growi-list-view.node.insert",
          "when": "view == GrowiList-page-content",
          "group": "navigation"
        },
        {
          "command": "growi-list-view.show-content",
          "when": "view == GrowiList-page-content",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "growi-list-view.node.insert",
          "when": "view == GrowiList-page-content",
          "group": "01_file"
        },
        {
          "command": "growi-list-view.node.indent",
          "when": "view == GrowiList-page-content",
          "group": "02_node"
        },
        {
          "command": "growi-list-view.node.outdent",
          "when": "view == GrowiList-page-content",
          "group": "02_node"
        },
        {
          "command": "growi-list-view.node.edit",
          "when": "view == GrowiList-page-content",
          "group": "inline@0"
        },
        {
          "command": "growi-list-view.node.delete",
          "when": "view == GrowiList-page-content",
          "group": "inline@1"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "GrowiListPlugin",
        "contents": "Input your token\n[input](command:growi-list-view.save-token)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "GrowiListPlugin",
          "title": "GrowiList",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "GrowiListPlugin": [
        {
          "id": "growi-list-page-list",
          "name": "GrowiList - pages",
          "icon": "resources/icon.svg"
        },
        {
          "id": "growi-list-page-content",
          "name": "GrowiList - Content",
          "icon": "resources/icon.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5"
  }
}